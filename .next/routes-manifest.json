{
  "version": 3,
  "pages404": true,
  "caseSensitive": false,
  "basePath": "",
  "redirects": [
    {
      "source": "/:path+/",
      "destination": "/:path+",
      "internal": true,
      "statusCode": 308,
      "regex": "^(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))/$"
    }
  ],
  "headers": [],
  "rewrites": {
    "beforeFiles": [],
    "afterFiles": [],
    "fallback": []
  },
  "dynamicRoutes": [
    {
      "page": "/api/issues/[id]",
      "regex": "^/api/issues/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPid": "nxtPid"
      },
      "namedRegex": "^/api/issues/(?<nxtPid>[^/]+?)(?:/)?$"
    }
  ],
  "staticRoutes": [
    {
      "page": "/",
      "regex": "^/(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/(?:/)?$"
    },
    {
      "page": "/_not-found",
      "regex": "^/_not\\-found(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/_not\\-found(?:/)?$"
    },
    {
      "page": "/dashboard/citizen",
      "regex": "^/dashboard/citizen(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/dashboard/citizen(?:/)?$"
    },
    {
      "page": "/dashboard/citizen/report",
      "regex": "^/dashboard/citizen/report(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/dashboard/citizen/report(?:/)?$"
    },
    {
      "page": "/dashboard/city-admin",
      "regex": "^/dashboard/city\\-admin(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/dashboard/city\\-admin(?:/)?$"
    },
    {
      "page": "/dashboard/department-admin",
      "regex": "^/dashboard/department\\-admin(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/dashboard/department\\-admin(?:/)?$"
    },
    {
      "page": "/dashboard/field-worker",
      "regex": "^/dashboard/field\\-worker(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/dashboard/field\\-worker(?:/)?$"
    },
    {
      "page": "/dashboard/field-worker/issues",
      "regex": "^/dashboard/field\\-worker/issues(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/dashboard/field\\-worker/issues(?:/)?$"
    },
    {
      "page": "/dashboard/regional-admin",
      "regex": "^/dashboard/regional\\-admin(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/dashboard/regional\\-admin(?:/)?$"
    },
    {
      "page": "/dashboard/super-admin",
      "regex": "^/dashboard/super\\-admin(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/dashboard/super\\-admin(?:/)?$"
    },
    {
      "page": "/login",
      "regex": "^/login(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/login(?:/)?$"
    },
    {
      "page": "/register",
      "regex": "^/register(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/register(?:/)?$"
    }
  ],
  "dataRoutes": [],
  "rsc": {
    "header": "rsc",
    "varyHeader": "rsc, next-router-state-tree, next-router-prefetch, next-router-segment-prefetch",
    "prefetchHeader": "next-router-prefetch",
    "didPostponeHeader": "x-nextjs-postponed",
    "contentTypeHeader": "text/x-component",
    "suffix": ".rsc",
    "prefetchSuffix": ".prefetch.rsc",
    "prefetchSegmentHeader": "next-router-segment-prefetch",
    "prefetchSegmentSuffix": ".segment.rsc",
    "prefetchSegmentDirSuffix": ".segments"
  },
  "rewriteHeaders": {
    "pathHeader": "x-nextjs-rewritten-path",
    "queryHeader": "x-nextjs-rewritten-query"
  }
}